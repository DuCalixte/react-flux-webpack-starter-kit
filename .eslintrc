{
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "flow-vars",
    "react",
    "jsx-a11y"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "_": true,
    "React": true,
    "Router": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "jsx-a11y/anchor-has-content": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "camelcase": ["off"],
    "comma-dangle": [0],
    "no-var": 0,
    "no-undef": 0,
    "no-console": 0,
    "newline-after-var": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "max-len": "off",
    "react/prefer-stateless-function": 0,
    "react/prop-types": 1,
    "react/forbid-prop-types": [0],
    "no-underscore-dangle": ["off"],
    "vars-on-top": 1
  }
}
