{
  "name": "react-flux-webpack-starter-kit",
  "version": "0.0.0",
  "description": "This is a react starter kit for the flux architecture with webpack and webpack-dev-server. It comes with enzyme and mocha for testing.",
  "main": "index.html",
  "config": {
    "port": 8080,
    "webpackPort": 8090
  },
  "scripts": {
    "clean": "rm -rf public",
    "build": "webpack --config app/config/webpack.config.js --debug",
    "reset": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "start": "webpack-dev-server --config app/config/webpack-dev-server.config.js --port ${PORT-8080}",
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuCalixte/react-flux-webpack-starter-kit.git"
  },
  "keywords": [
    "react",
    "flux",
    "webpack",
    "webpack-dev-server",
    "enzyme",
    "mocha",
    "starter-kit"
  ],
  "author": "Stanley Calixte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuCalixte/react-flux-webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/DuCalixte/react-flux-webpack-starter-kit#readme",
  "dependencies": {
    "alt": "^0.18.6",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bootstrap-loader": "^1.3.3",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "expose-loader": "^0.7.1",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.1",
    "history": "^4.5.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "node-sass": "^3.13.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "superagent": "^3.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "fs": "0.0.1-security",
    "json-loader": "^0.5.4",
    "webpack-build-notifier": "^0.1.13",
    "webpack-dev-server": "^1.16.2"
  }
}
