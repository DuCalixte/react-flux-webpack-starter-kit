{
  "name": "react-flux-webpack-starter-kit",
  "version": "0.0.0",
  "description": "This is a react starter kit for the flux architecture with webpack and webpack-dev-server. It comes with enzyme and mocha for testing.",
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf public",
    "build": "webpack --config app/config/webpack.config.js --debug",
    "reset": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "start": "node app/config/webpack.dev.server.config.js",
    "no-start": "webpack-dev-server --progress --config app/config/webpack-dev-server.config.js --port ${PORT-8080}",
    "deploy": "npm run clean && NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "node_modules/karma-cli/bin/karma start app/config/karma.conf.js",
    "watch-test": "npm run test -- --single-run=false",
    "lint": "eslint app",
    "lintfix": "eslint app --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuCalixte/react-flux-webpack-starter-kit.git"
  },
  "keywords": [
    "react",
    "flux",
    "webpack",
    "webpack-dev-server",
    "enzyme",
    "mocha",
    "starter-kit"
  ],
  "author": "Stanley Calixte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuCalixte/react-flux-webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/DuCalixte/react-flux-webpack-starter-kit#readme",
  "dependencies": {
    "alt": "^0.18.6",
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bootstrap-loader": "^1.3.3",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-react": "^6.9.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.1",
    "history": "^4.5.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "node-sass": "^3.13.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "superagent": "^3.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.14.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-import": "^2.2.0",
    "faker": "^3.1.0",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai-latest": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack-build-notifier": "^0.1.13",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0"
  }
}
